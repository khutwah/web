{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "tsx --test ./src/**/*.test.ts",
    "dev": "next dev --turbopack",
    "ladle": "ladle serve --viteConfig ladle-vite.config.ts",
    "ladle:build": "ladle build --viteConfig ladle-vite.config.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:sync": "snaplet-seed sync",
    "seed": "tsx seed/setup.ts",
    "dbtypes:generate": "supabase gen types --lang=typescript --local > src/models/database.types.ts && prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prepare": "husky",
    "format": "prettier . --write",
    "sql:format": "find . -type f -name \"*.sql\" -exec sql-formatter -l postgresql --fix {} ';'",
    "type:check": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-checkbox": "1.1.2",
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "1.2.2",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.46.1",
    "class-variance-authority": "0.7.0",
    "clsx": "^2.1.1",
    "cookies-next": "^5.0.2",
    "dayjs": "^1.11.13",
    "lucide-react": "0.456.0",
    "next": "15.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.53.2",
    "recharts": "2.14.1",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "title-case": "^4.3.2",
    "use-debounce": "^10.0.4",
    "vaul": "^1.1.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@ladle/react": "^4.0.3",
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "0.98.0",
    "@types/node": "^20.17.10",
    "@types/pg": "^8.11.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "patch-package": "^8.0.0",
    "pg": "^8.13.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0",
    "prisma": "^6.1.0",
    "sql-formatter": "^15.4.8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "overrides": {
    "@ladle/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-hook-form": {
      "react": "$react"
    },
    "react-remove-scroll": {
      "react": "$react"
    },
    "react-remove-scroll-bar": {
      "react": "$react"
    }
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.x"
  }
}
