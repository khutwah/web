{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "ladle": "ladle serve --viteConfig ladle-vite.config.ts",
    "ladle:build": "ladle build --viteConfig ladle-vite.config.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "NODE_OPTIONS='-r dotenv/config' npx tsx seed.ts",
    "db-types": "supabase gen types --lang=typescript --local > src/models/database.types.ts",
    "prepare": "husky",
    "format": "npx prettier . --write",
    "type:check": "npx tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "1.1.2",
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-toast": "1.2.2",
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.46.1",
    "class-variance-authority": "0.7.0",
    "classnames": "2.5.1",
    "dayjs": "^1.11.13",
    "lucide-react": "0.456.0",
    "next": "15.0.2",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "react-hook-form": "7.53.2",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@ladle/react": "^4.0.3",
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "^0.98.0",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "pg": "^8.13.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "overrides": {
    "@ladle/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-hook-form": {
      "react": "$react"
    },
    "react-remove-scroll": {
      "react": "$react"
    },
    "react-remove-scroll-bar": {
      "react": "$react"
    }
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.x"
  }
}
